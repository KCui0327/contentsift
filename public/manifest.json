{
  "name": "welovewill",
  "description": "welovewill",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  "icons": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
  },
  "content_scripts": [
    {
        "matches": [
            "https://twitter.com/*",
            "https://www.facebook.com/*",
            "https://www.instagram.com/*"
        ],
        "js": [
            "./scripts/content.js"
        ],
        "run_at": "document_end"
    }
]
}